// Include library
#include <ESP8266WiFi.h>

// Define LED pin
const int ledPin = D1;

void setup() {
  // Set the LED pin as an output
  pinMode(ledPin, OUTPUT);
}

void loop() {
  // Turn the LED on
  digitalWrite(ledPin, HIGH);
  delay(1000); // Tunggu selama 1 detik

  // Matikan LED
  digitalWrite(ledPin, LOW);
  delay(1000); // Tunggu selama 1 detik lagi
}
